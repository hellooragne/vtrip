                                <div class="day_scene_list border_none">
                                    
                                        <div class="scene_pic">
                                            
                                            <a poiid="10524230" sight="true" href="###">
                                              <img poiid="10524230" width="210px" height="118px" title="悉尼鱼市" src="http://youimg1.c-ctrip.com/target/tg/247/659/928/45fb2ac8657448d18cd4cf3ecb346a3c.jpg" />
