            var _key = "vacation.Detail", //绑定的key,需要申请 	
 	                   _param = {
 	                       "UID":"${duid}","page_id":"${page_id}","ProductID":"2376781","From":"上海","To":"悉尼","ProductType":"团队游","Version":"1"
 	                   },
 	                   getParam = function (pVal) {
 	                       var _arr = [];
 	                       for (var name in pVal) {
 	                           _arr.push([name, "=", pVal[name]].join(""));
 	                       }
 	                       return _arr.join("&");
 	                   },
 	                   _clock = setInterval(function () {
 	                       if (window['$_bf'] && window['$_bf'].loaded === true) {
 	                           window.$_bf.tracklog(_key, getParam(_param));
 	                           clearInterval(_clock);
 	                       }
 	                   }, 1000);
     })(); 
